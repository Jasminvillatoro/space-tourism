generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Destination {
  id          Int     @id @default(autoincrement())
  name        String
  href        String
  images      String?
  description String
  distance    String
  travel      String
}

model Crew {
  id     Int     @id @default(autoincrement())
  name   String
  images String?
  role   String
  bio    String
}

model Technology {
  id          Int     @id @default(autoincrement())
  name        String
  images      String?
  description String
}
